        //lnk_filter0   Constructions   Labour
        $(".lnk_filter0").click(function() {
            $(".constructions, .labour, .educational").show();
        });
        $(".lnk_filter1").click(function() {
            $(".labour").show();
            $(".constructions").hide();
            $(".educational").hide();
        });
        $(".lnk_filter2").click(function() {
            $(".labour").hide();
            $(".constructions").show();
            $(".educational").hide();
        });
        $(".lnk_filter3").click(function() {
            $(".labour").hide();
            $(".constructions").hide();
            $(".educational").show();
        });


        //Font Increse-decrease
        var $affectedElements = $("p, h1, h2, h3, h4, h5, h6, span, small"); // Can be extended, ex. $("div, p, span.someClass")

        // Storing the original size in a data attribute so size can be reset
        $affectedElements.each(function() {
            var $this = $(this);
            $this.data("orig-size", $this.css("font-size"));
        });

        $("#btn-increase").click(function() {
            changeFontSize(1);
        });

        $("#btn-decrease").click(function() {
            changeFontSize(-1);
        });

        $("#btn-orig").click(function() {
            $affectedElements.each(function() {
                var $this = $(this);
                $this.css("font-size", $this.data("orig-size"));
            });
        });

        function changeFontSize(direction) {
            $affectedElements.each(function() {
                var $this = $(this);
                $this.css("font-size", parseInt($this.css("font-size")) + direction);
            });
        }
